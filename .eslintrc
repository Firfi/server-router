--- # Lint configuration in YAML
  extends: airbnb
  parser: babel-eslint
  rules:
    class-methods-use-this: 0
    import/extensions: 0
    import/no-extraneous-dependencies: 0
    import/no-unresolved: 0
    import/prefer-default-export: 0
    no-underscore-dangle:
      - error
      - allowAfterThis: true
    no-use-before-define: 0
  globals:
    _: false
    $PropertyType: false
    InjectData: false
